{
  /* Set the desired framerate for the renderer and the encoder. */
  "serverFrameCaptureFPS": 60,
  "NvencodeSettings": {
    /* Setup the average and min bitrate for the encoder.  
    * WebRTC will modify the bitrate based on bandwidth but will never go below minBitrate.
    * WARNING: Setting a high minBitrate can cause the stream to freeze. */
    "bitrate": 5500000,
    "minBitrate": 3500000,
    /* Setup an encoder that puts an IDR every 60 frames, an I-frame every 15
    * frames, and the rest P-frames. 
    * Set intraRefreshEnableFlag to false to disable I-frames. */
    "idrPeriod": 60,
    "intraRefreshPeriod": 15,
    "intraRefreshEnableFlag": true
  }
}